package gui;

import java.awt.BorderLayout;
import java.awt.FlowLayout;

import javax.swing.JButton;
import javax.swing.JDialog;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;
import javax.swing.JScrollPane;
import javax.swing.JTable;

public class dlgProducto extends JDialog {

	private static final long serialVersionUID = 1L;
	private JTable tblTable;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		try {
			dlgProducto dialog = new dlgProducto();
			dialog.setDefaultCloseOperation(JDialog.DISPOSE_ON_CLOSE);
			dialog.setVisible(true);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

	/**
	 * Create the dialog.
	 */
	public dlgProducto() {
		setBounds(100, 100, 606, 550);
		getContentPane().setLayout(null);
		{
			JScrollPane scrollPane = new JScrollPane();
			scrollPane.setBounds(10, 174, 572, 329);
			getContentPane().add(scrollPane);
			{
				tblTable = new JTable();
				tblTable.setFillsViewportHeight(true);
				scrollPane.setViewportView(tblTable);
			}
		}
	}

}
